{
    "compilerOptions": {
      "target": "ES2021",
      "module": "ESNext",
      "lib": ["DOM", "DOM.Iterable", "ESNext"],
      "moduleResolution": "Bundler",
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "resolveJsonModule": true,
      "isolatedModules": true,
      "jsx": "react-jsx",
      "noEmit": true,
      "strict": true,
      "skipLibCheck": true,
      "incremental": true,
      "allowJs": true
    }
  }